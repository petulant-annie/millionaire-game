{
  "name": "millionaire-js",
  "version": "0.1.0",
  "description": "Who Wants to Be a Millionaire?",
  "main": "index.tsx",
  "homepage": "https://petulant-annie.github.io/millionaire-game",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode none",
    "build": "webpack --config webpack.config.js",
    "test": "run lint && npm run test:run",
    "test:run": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .tsx",
    "lint:fix": "npm run lint -- --fix",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": "3.22"
        }
      ]
    ],
    "plugins": []
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "^29.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.6.0",
    "react-scripts": "5.0.1",
    "web-vitals": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.7",
    "@babel/preset-env": "7.20.2",
    "@svgr/webpack": "6.5.1",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "babel-loader": "9.1.0",
    "babel-preset-solid": "1.6.6",
    "eslint": "8.30.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-standard-with-typescript": "24.0.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.6.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.11",
    "gh-pages": "^5.0.0",
    "html-webpack-plugin": "5.5.0",
    "ts-loader": "9.4.2",
    "tslib": "^2.4.1",
    "typescript": "4.9.4",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1"
  }
}
